<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
  <KeyBinding x:Key="GoToLineKeyBinding" Gesture="CTRL+G" />
  <KeyBinding x:Key="FindWhatKeyBinding" Gesture="CTRL+F" />
  <KeyBinding x:Key="OpenFileKeyBinding" Gesture="CTRL+O" />
  <KeyBinding x:Key="OpenFileManagerKeyBinding" Gesture="CTRL+M" />
  <KeyBinding x:Key="ClearTailWindowKeyBinding" Gesture="CTRL+E" />
  <KeyBinding x:Key="StartTailKeyBinding" Gesture="CTRL+R" />
  <KeyBinding x:Key="StopTailKeyBinding" Gesture="CTRL+U" />
  <KeyBinding x:Key="CopyLineKeyBinding" Gesture="CTRL+C" />
  <KeyBinding x:Key="NewTabItemKeyBinding" Gesture="CTRL+T" />
  <KeyBinding x:Key="CloseTabItemKeyBinding" Gesture="CTRL+W" />
  <KeyBinding x:Key="ToggleAlwaysOnTopKeyBinding" Gesture="ALT+T" />
  <KeyBinding x:Key="ToggleFilterKeyBinding" Gesture="ALT+F" />
  <KeyBinding x:Key="SearchForwardKeyBinding" Gesture="F3" />
  <KeyBinding x:Key="MinimizeWndKeyBinding" Gesture="CTRL+ALT+M" />
  <KeyBinding x:Key="QuickSearchKeyBinding" Gesture="CTRL+Q" />
  <KeyBinding x:Key="QuickSaveKeyBinding" Gesture="CTRL+S" />
</ResourceDictionary>
