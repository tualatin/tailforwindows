<Window
    x:Class="Org.Vs.TailForWin.UI.T4Window"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Org.Vs.TailForWin.UI"
    xmlns:viewModel="clr-namespace:Org.Vs.TailForWin.UI.ViewModels"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance {x:Type viewModel:T4WindowViewModel}}"
    Title="{Binding WindowTitle}"
    Width="{Binding Width, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
    Height="{Binding Height, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
    >
    <Grid />
</Window>
